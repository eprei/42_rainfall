{
  int user_input;
  char *command;
  undefined4 local_1c;
  __uid_t uid;
  __gid_t gid;
  
  user_input = atoi(*(char **)(param_2 + 4));
  if (user_input == 0x1a7) {
    command = strdup("/bin/sh");
    local_1c = 0;
    gid = getegid();
    uid = geteuid();
